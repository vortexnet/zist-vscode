{
  "name": "zist-vscode",
  "displayName": "zist-vscode",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "zist-vscode-activitybar",
          "title": "Zist-vscode",
          "icon": "media/snippet.svg"
        }
      ]
    },
    "views": {
      "zist-vscode-activitybar": [
        {
          "type": "webview",
          "id": "zist-vscode.sidebar",
          "name": "Zist",
          "icon": "media/snippet.svg",
          "contextualTitle": "Zist"
        },
        {
          "type": "webview",
          "id": "zist-vscode.sidebar-accordian",
          "name": "Zist-Snippets"
        }
      ]
    },
    "menus": {
      "view/title": [
        {
          "command": "zist-vscode.sidebar-accordian",
          "group": "navigation",
          "when": "view == zist-vscode.sidebar-accordian"
        }
      ]
    },
    "commands": [
      {
        "command": "zist-vscode.helloWorld",
        "title": "Hello World",
        "category": "Zist"
      },
      {
        "command": "zist-vscode.authenticate",
        "title": "Authenticate",
        "category": "Zist"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack --config ./build/node-extension.webpack.config.js",
    "watch": "concurrently \"rollup -c -w\" \"webpack --watch --config ./build/node-extension.webpack.config.js\"",
    "package": "webpack --mode production --devtool hidden-source-map --config ./build/node-extension.webpack.config.js",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "pnpm run compile && pnpm run lint",
    "test": "node ./out/test/runTest.js",
    "prepare": "husky install",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier \"src/**/*.{js,jsx,ts,tsx,css,scss}\" --write"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.1",
    "@rollup/plugin-typescript": "^8.1.0",
    "@tsconfig/svelte": "^1.0.10",
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "@types/polka": "^0.5.4",
    "@types/vscode": "^1.52.0",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "concurrently": "^5.3.0",
    "eslint": "^7.15.0",
    "eslint-plugin-prettier": "^5.0.0",
    "glob": "^7.1.6",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "mocha": "^8.1.3",
    "prettier": "^3.0.1",
    "rollup": "^2.3.4",
    "rollup-plugin-svelte": "^6.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "svelte": "^3.31.0",
    "svelte-check": "^1.1.23",
    "svelte-preprocess": "^4.6.1",
    "tcs": "^10.0.2",
    "ts-loader": "^8.0.11",
    "typescript": "^4.9.3",
    "vscode-test": "^1.4.1",
    "webpack": "^5.10.0",
    "webpack-cli": "^4.2.0"
  },
  "dependencies": {
    "polka": "^0.5.2"
  }
}
